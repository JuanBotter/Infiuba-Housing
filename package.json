{
  "name": "infiuba-alojamientos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "test": "vitest run",
    "build": "next build",
    "start": "next start",
    "import:data": "node scripts/build-dataset.mjs",
    "geocode:data": "node scripts/geocode-listings.mjs",
    "db:init": "npm run db:migrate",
    "db:seed": "node scripts/db-seed.mjs",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "db:migrate:down": "node scripts/db-migrate.mjs down",
    "user:upsert": "node scripts/user-upsert.mjs",
    "db:migrate": "node scripts/db-migrate.mjs",
    "vercel-build": "npm run test && npm run db:migrate && npm run build",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:integration:docker": "bash scripts/test-integration.sh"
  },
  "dependencies": {
    "@vercel/blob": "^2.2.0",
    "leaflet": "^1.9.4",
    "next": "^16.1.6",
    "pg": "^8.16.3",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.21",
    "@types/node": "20.14.15",
    "@types/pg": "^8.15.6",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitest/coverage-v8": "^4.0.18",
    "node-pg-migrate": "^8.0.4",
    "typescript": "5.5.4",
    "vitest": "^4.0.18"
  }
}
